			<success>Always</success>
			<xpath>/Defs/RimBees.BeeCombinationDef[defName="RB_CombinationTier2_1"]/result</xpath>
			<value>
				<li>Northern</li>
			</value>
		</match>
		<!-- If the first XPATH does not exist, the original code stands. -->
	</Operation>
</Patch>
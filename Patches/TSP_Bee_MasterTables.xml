		<match Class="PatchOperationAdd">
			<success>Always</success>
			<xpath>Defs/RimBees.BeeCombinationDef[defName="RB_CombinationTier2_Worker"]/result</xpath>
			<value>
				<li>Warden</li>
			</value>
		</match>
		<!-- If the first XPATH does not exist, the original code stands. -->
	</Operation>
	

		</match>
		<!-- If it DOES NOT, add this. -->
		<nomatch Class="PatchOperationAdd">
			<xpath>Defs/ThingDef[@Name="RS_VendingMachineBase"]</xpath>
			<success>Always</success>
			<value>
				<researchPrerequisites Inherit="False">

							<species>Gravlite</species>
							<comb>TSP_RB_Beeliever_Gravlite_Honeycomb</comb>
							<combtimedays>2</combtimedays>
							<nocturnal>true</nocturnal> <!-- Works at night -->
							<pluviophile>false</pluviophile> <!-- Works in the rain -->
							<weirdplantneeded>BlocksVacstone</weirdplantneeded>
							<tempMin>-270</tempMin> <!-- Negative four hundred fifty-five degrees, this is the temperature of outer space-->
							<tempMax>36</tempMax> <!-- Ninety-seven degrees, rounded -->
							<additionalBeeEffects Inherit="False">
								<li Class="RimBees.AdditionalBeeEffects_Repair" Inherit="False">
									<amount>7</amount>
								</li>
							</additionalBeeEffects>
						</li>
						<li Class="RimBees.CompProperties_TempRuinableAndDestroy" Inherit="False">
							<minSafeTemperature>-270</minSafeTemperature>
							<maxSafeTemperature>36</maxSafeTemperature>
							<progressPerDegreePerTick>0.000001</progressPerDegreePerTick>
						</li>
						<li Inherit="False">
							<compClass>RimBees.CompDangerBee</compClass>
						</li>
					</comps>
				</ThingDef>
				
